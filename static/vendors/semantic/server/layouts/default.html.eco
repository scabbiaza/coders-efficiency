  <script type="text/javascript">
    window.liveSettings = {
      api_key    : '9ede3015b9f84c1aabc81ab839c55d74',
      parse_attr : [
        'data-title',
        'data-content'
      ],
      detectlang   : true,
      autocollect  : true,
      ignore_tags  : ['i'],
